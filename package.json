{
  "name": "mongo-func",
  "version": "0.0.1",
  "description": "A functional approach to MongoDB",
  "main": "lib/index.js",
  "scripts": {
    "build": "./node_modules/.bin/babel src --out-dir lib",
    "hint": "./node_modules/.bin/jshint src/* && ./node_modules/.bin/jshint test/*",
    "test": "npm run build && npm run hint && mocha --require blanket --reporter mocha-lcov-reporter | ./node_modules/coveralls/bin/coveralls.js",
    "test-local": "npm run build && npm run hint && mocha"
  },
  "engines": {
    "node": ">= 0.10.x"
  },
  "dependencies": {
    "mongodb": "2.0.42",
    "ramda": "0.17.1",
    "babel": "5.8.21"
  },
  "devDependencies": {
    "mocha": "2.2.5",
    "coveralls": "~2.11.4",
    "mocha-lcov-reporter": "0.0.2",
    "blanket": "~1.1.7",
    "jshint": "~2.8.0"
  },
  "config": {
    "blanket": {
      "pattern": [
        "lib"
      ],
      "data-cover-never": [
        "node_modules",
        "tests"
      ]
    }
  },
  "author": "",
  "repository": {
    "type": "git",
    "url": "git@github.com:scottie1984/mongo-func.git"
  },
  "license": "BSD-2-Clause",
  "readmeFilename": "README.md",
  "bugs": {
    "url": "https://github.com/scottie1984/mongo-func/issues"
  }
}
